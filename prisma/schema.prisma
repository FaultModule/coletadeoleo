generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id            Int      @id @default(autoincrement())
  first_name    String
  last_name     String
  email         String   @unique
  phone         String
  password_hash String
  address       String
  cep           String
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt

  @@map("users")
}
